{
  "name": "@fhe-gbdt/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for FHE-GBDT - Privacy-Preserving GBDT Inference",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "fhe",
    "gbdt",
    "privacy",
    "machine-learning",
    "homomorphic-encryption",
    "xgboost",
    "lightgbm",
    "catboost",
    "differential-privacy"
  ],
  "author": "FHE-GBDT Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/fhe-gbdt-serving.git",
    "directory": "sdk/typescript"
  },
  "bugs": {
    "url": "https://github.com/example/fhe-gbdt-serving/issues"
  },
  "homepage": "https://github.com/example/fhe-gbdt-serving#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.3"
  }
}
